<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox prefHeight="506.0" prefWidth="557.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.vocabularytms.VocabularyController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <children>
        <HBox spacing="10.0">
            <TextField fx:id="textValue" prefHeight="29.0" prefWidth="300.0">
                <font>
                    <Font name="Agency FB Bold" size="14.0" />
                </font>
            </TextField>
            <Button fx:id="add" mnemonicParsing="false" onAction="#addWord" prefHeight="25.0" text="Add word" />
            <Button fx:id="delete" mnemonicParsing="false" onAction="#deleteWord" prefHeight="25.0" text="Delete word" />
            <Button fx:id="close" mnemonicParsing="false" onAction="#closeApp" prefHeight="25.0" text="Close App"/>
        </HBox>
        <Button fx:id="save" mnemonicParsing="false" onAction="#saveVocabulary" prefHeight="25.0" prefWidth="103.0"
                text="Save changes" />
        <ListView fx:id="listView" prefHeight="236.0" prefWidth="298.0" />

    </children>
</VBox>
